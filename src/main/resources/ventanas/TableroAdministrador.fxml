<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="700.0" prefWidth="270.0" styleClass="white-color" stylesheets="@../aplicacion.css" xmlns="http://javafx.com/javafx/17.0.6" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.storify.controllers.TableroAdminController">
   <children>
      <ImageView fx:id="btnCanciones" fitHeight="150.0" fitWidth="150.0" layoutX="58.0" layoutY="352.0" onMouseClicked="#verCancionesGenerales">
         <image>
            <Image url="@../imagenes/Musica Icon.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fx:id="btnGestionarArtistas" fitHeight="175.0" fitWidth="175.0" layoutX="59.0" layoutY="19.0" onMouseClicked="#gestionarArtistas">
         <image>
            <Image url="@../imagenes/Gestionar Artistas Icon.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fx:id="btnAnaliticas" fitHeight="150.0" fitWidth="150.0" layoutX="58.0" layoutY="509.0" onMouseClicked="#verEstadisticas">
         <image>
            <Image url="@../imagenes/Analytics Icon.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fx:id="btnGestionarCanciones" fitHeight="175.0" fitWidth="175.0" layoutX="58.0" layoutY="176.0" onMouseClicked="#gestionarCanciones">
         <image>
            <Image url="@../imagenes/Gestionar Canciones Icon.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
   </children>
</Pane>
